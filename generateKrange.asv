function [k_range,len_krange] = generateKrange (m,n, limits_values, k_stride, k_possible_max)

if nargin < 5
    k_max = min(m,n)-1;
else
    k_max = min(k_possible_max, min(m,n)-1); 
end

range = [max(limits_values )+ k_stride:k_stride:k_max];
range = cat(1, range, k_max);

k_range = cat(1, limits_values, range);
len_krange = length(k_range);